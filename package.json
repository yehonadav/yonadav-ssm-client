  "license": "Apache-2.0",
  "repository": "yehonadav/yonadav-ssm-client",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "build:version": "npm run build && npm version patch && npm publish --access=public",
    "update:version": "git add . && git commit -am \"update\" && git push && npm run build:version",
    "i:build:version": "npm i && npm run build:version",
    "i:update:version": "npm i && npm run update:version"
  },
  "dependencies": {
    "@yehonadav/apierror": "^0.0.1",
    "@yehonadav/statuscodes": "^1.0.6",
    "@yehonadav/validations": "^0.0.1",
    "axios": "^0.26.0",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "rollup": "^1.32.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "common",
    "components",
    "typescript",
    "npm"
  ]
}
